var arr=[1,3,5,7,9];
var arr1=[2,4,6,8];
var res=[...arr,...arr1].sort("");
var n=res.length;
var array=function (res,n){

var result=Math.floor(n/2);
var final=res[result];
return final;
}
console.log(array(res,n));


