var nums=[1,2,3,4];
var k=3;
var rotate= function(nums, k) {

 for (let i = 0; i < k; i++) {
     nums.unshift(nums.pop());
 }

 return nums;
}
console.log(rotate(nums,k));


var rotate= (function(nums, k) {

  for (let i = 0; i < k; i++) {
      nums.unshift(nums.pop());
  }

  return nums;
})([1,2,3,4],3);



